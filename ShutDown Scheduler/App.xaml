<Application x:Class="ShutDown_Scheduler.App"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:local="clr-namespace:ShutDown_Scheduler"
            xmlns:viewmodel="clr-namespace:ShutDown_Scheduler.MVVM.ViewModel"
            xmlns:tb="http://www.hardcodet.net/taskbar"
            StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/CountdownNumber.xaml"/>
                <ResourceDictionary Source="Theme/Buttons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
